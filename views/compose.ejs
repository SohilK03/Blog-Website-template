<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="form" action="/compose" method="post">
    
<div class="form-group"></div>
    <label for="title">Title</label>
    <input class="form-control" type="text" name="title" id="" autocomplete="off">
    
    <div class="form-group">
        <label for="PostCOntent">Post</label>
        <textarea name="PostContent" class="form-control" autocomplete="off" rows="5"></textarea>
    </div>
    <button class="btn btn-lg btn-primary" type="submit">Publish</button>
</form>

<%- include("partials/footer") -%>